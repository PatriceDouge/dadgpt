{
  "name": "dadgpt",
  "version": "0.1.0",
  "description": "Personal CLI AI assistant for managing goals, todos, and family life",
  "type": "module",
  "bin": {
    "dadgpt": "./dist/bin/dadgpt.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun run build:compile && bun run build:bin",
    "build:compile": "bun build src/index.ts --outdir dist --target node --format esm",
    "build:bin": "mkdir -p dist/bin && echo '#!/usr/bin/env node' > dist/bin/dadgpt.js && echo 'import \"../index.js\";' >> dist/bin/dadgpt.js",
    "test": "bun test",
    "lint": "bun tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "cli",
    "ai",
    "assistant",
    "goals",
    "todos",
    "productivity",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "personal-assistant"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/dadgpt.git"
  },
  "homepage": "https://github.com/yourusername/dadgpt#readme",
  "bugs": {
    "url": "https://github.com/yourusername/dadgpt/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.9",
    "@ai-sdk/openai": "^3.0.7",
    "ai": "^6.0.27",
    "chalk": "^5.6.2",
    "ora": "^9.0.0",
    "ulid": "^3.0.2",
    "xstate": "^5.25.0",
    "yargs": "^18.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/yargs": "^17.0.35",
    "typescript": "^5.9.3"
  }
}
